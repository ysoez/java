<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%yellow(%d{HH:mm:ss.SSS}) %green([%thread]) %cyan(%-5level) %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="io.netty" level="WARN"/>
    <root level="debug">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
